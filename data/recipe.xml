    <recipes>
        <name>Chicken Rice</name>
        <difficulty>3</difficulty>
        <cooktime>40M</cooktime>
        <instructions>
            <instruction>Slice and clean up the inner of the chicken 1.2 kg.</instruction>
            <ingredients>
                <name>chicken</name>
                <amount>1.2</amount>
                <unit>kg</unit>
            </ingredients>
        </instructions>
        <instructions>
            <instruction>Boil the chicken in water 700 ml.</instruction>
            <ingredients>
                <name>water</name>
                <amount>700.0</amount>
                <unit>ml</unit>
            </ingredients>
        </instructions>
        <instructions>
            <instruction>Remove 200 ml of water and put soy sauce 100 ml.</instruction>
            <ingredients>
                <name>soy sauce</name>
                <amount>100.0</amount>
                <unit>ml</unit>
            </ingredients>
        </instructions>
        <instructions>
            <instruction>Cook for 20 mins.</instruction>
            <cooktime>20M</cooktime>
        </instructions>
        <tagged>Singapore</tagged>
        <tagged>Poultry</tagged>
        <tagged>Asian</tagged>
    </recipes>
