    <recipes>
        <name>Roti Prata</name>
        <difficulty>3</difficulty>
        <cooktime>40M</cooktime>
        <instructions>
            <instruction>Sift flour, add salt and water to make dough.</instruction>
            <ingredients>
                <name>water</name>
                <amount>0.5</amount>
                <unit>cup</unit>
            </ingredients>
            <ingredients>
                <name>ghee</name>
                <amount>0.5</amount>
                <unit>cup</unit>
            </ingredients>
            <ingredients>
                <name>flour</name>
                <amount>300.0</amount>
                <unit>g</unit>
            </ingredients>
        </instructions>
        <instructions>
            <instruction>Knead thoroughly for 5 minutes.</instruction>
            <cooktime>5M</cooktime>
        </instructions>
        <instructions>
            <instruction>Heat one teaspoon of ghee or butter on a metal griddle or heavy iron pan.</instruction>
        </instructions>
        <instructions>
            <instruction>Fry each prata dough until brown on both sides.</instruction>
        </instructions>
        <tagged>Vegetarian</tagged>
        <tagged>Halal</tagged>
        <tagged>Staple</tagged>
    </recipes>
    <recipes>
        <name>Cheese Fries</name>
        <difficulty>2</difficulty>
        <cooktime>40M</cooktime>
        <instructions>
            <instruction>Melt butter on low.</instruction>
            <ingredients>
                <name>butter</name>
                <amount>0.25</amount>
                <unit>cup</unit>
            </ingredients>
        </instructions>
        <instructions>
            <instruction>Add milk. Add cheese slowly, while stirring.</instruction>
            <ingredients>
                <name>cheese</name>
                <amount>450.0</amount>
                <unit>g</unit>
            </ingredients>
            <ingredients>
                <name>milk</name>
                <amount>0.75</amount>
                <unit>cup</unit>
            </ingredients>
        </instructions>
        <instructions>
            <instruction>Simmer for 20 minutes, stirring often.</instruction>
            <cooktime>20M</cooktime>
        </instructions>
        <instructions>
            <instruction>Add salt and cornstarch mix.</instruction>
            <ingredients>
                <name>salt</name>
                <amount>1.0</amount>
                <unit>teaspoon</unit>
            </ingredients>
            <ingredients>
                <name>cornstarch</name>
                <amount>0.5</amount>
                <unit>teaspoon</unit>
            </ingredients>
        </instructions>
        <instructions>
            <instruction>Simmer another 10 minutes, stirring often</instruction>
            <cooktime>10M</cooktime>
        </instructions>
        <instructions>
            <instruction>Serve on top of fries</instruction>
        </instructions>
        <tagged>Western</tagged>
        <tagged>Vegetarian</tagged>
        <tagged>Halal</tagged>
    </recipes>
    <recipes>
        <name>Chicken Kebab</name>
        <difficulty>3</difficulty>
        <cooktime>30M</cooktime>
        <instructions>
            <instruction>Heat grill to medium-high. In a bowl, toss the chicken, garlic, thyme, oil and 1/2 tsp salt and pepper each. Thread the chicken on the skewers.</instruction>
            <ingredients>
                <name>olive oil</name>
                <amount>1.0</amount>
                <unit>tablespoon</unit>
            </ingredients>
            <ingredients>
                <name>garlic</name>
                <amount>2.0</amount>
                <unit>clove</unit>
            </ingredients>
            <ingredients>
                <name>pepper</name>
                <amount>0.5</amount>
                <unit>teaspoon</unit>
            </ingredients>
            <ingredients>
                <name>salt</name>
                <amount>0.5</amount>
                <unit>teaspoon</unit>
            </ingredients>
            <ingredients>
                <name>chicken</name>
                <amount>680.0</amount>
                <unit>g</unit>
            </ingredients>
            <ingredients>
                <name>thyme</name>
                <amount>1.0</amount>
                <unit>tablespoon</unit>
            </ingredients>
        </instructions>
        <instructions>
            <instruction>Grill the chicken, turning occasionally, until the chicken is cooked through 7 to 10 minutes.</instruction>
            <cooktime>10M</cooktime>
        </instructions>
        <tagged>Poultry</tagged>
        <tagged>Halal</tagged>
        <tagged>Staple</tagged>
    </recipes>
    <recipes>
        <name>Egg Fried Rice</name>
        <difficulty>3</difficulty>
        <cooktime>30M</cooktime>
        <instructions>
            <instruction>Heat the vegetable oil in a wok, add the onion and leek and fry gently until softened but not browned.</instruction>
            <ingredients>
                <name>onion</name>
                <amount>5.0</amount>
                <unit>g</unit>
            </ingredients>
            <ingredients>
                <name>leek</name>
                <amount>150.0</amount>
                <unit>g</unit>
            </ingredients>
            <ingredients>
                <name>vegetable oil</name>
                <amount>0.125</amount>
                <unit>teaspoon</unit>
            </ingredients>
        </instructions>
        <instructions>
            <instruction>Add the eggs and whisk until they are beginning to scramble and cook. Add the soy sauce and whisk.</instruction>
            <ingredients>
                <name>soy sauce</name>
                <amount>2.0</amount>
                <unit>tablespoon</unit>
            </ingredients>
            <ingredients>
                <name>eggs</name>
                <amount>2.0</amount>
                <unit>whole</unit>
            </ingredients>
        </instructions>
        <instructions>
            <instruction>Add the cooked rice and stir well to coat in the eggs. Stir in the chopped chives, salt and freshly ground black pepper and spoon into a serving dish. Drizzle with soy sauce and serve.</instruction>
            <ingredients>
                <name>salt</name>
                <amount>0.5</amount>
                <unit>tablespoon</unit>
            </ingredients>
            <ingredients>
                <name>black pepper</name>
                <amount>0.5</amount>
                <unit>tablespoon</unit>
            </ingredients>
            <ingredients>
                <name>rice</name>
                <amount>200.0</amount>
                <unit>g</unit>
            </ingredients>
            <ingredients>
                <name>chives</name>
                <amount>1.0</amount>
                <unit>tablespoon</unit>
            </ingredients>
        </instructions>
        <tagged>Staple</tagged>
        <tagged>Asian</tagged>
    </recipes>
