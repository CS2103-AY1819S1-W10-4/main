    <recipes>
        <name>Beef Steak With Butter Rice</name>
        <difficulty>4</difficulty>
        <cooktime>30M</cooktime>
        <instructions>
            <instruction>Leave beef exposed to room temperature for 15M</instruction>
            <cooktime>15M</cooktime>
            <ingredients>
                <name>beef</name>
                <amount>1.0</amount>
                <unit>piece</unit>
            </ingredients>
        </instructions>
        <instructions>
            <instruction>Season beef lightly with pinch of salt and pepper</instruction>
            <ingredients>
                <name>salt</name>
                <amount>1.0</amount>
                <unit>pinch</unit>
            </ingredients>
            <ingredients>
                <name>blackpepper</name>
                <amount>1.0</amount>
                <unit>pinch</unit>
            </ingredients>
        </instructions>
        <instructions>
            <instruction>Heat one teaspoon butter on a pan.</instruction>
            <ingredients>
                <name>butter</name>
                <amount>1.0</amount>
                <unit>teaspoon</unit>
            </ingredients>
        </instructions>
        <instructions>
            <instruction>Sear the beef on medium heat, starting from the fattier parts, then cook until browned.</instruction>
            <cooktime>10M</cooktime>
        </instructions>
        <instructions>
            <instruction>Lightly glaze wok with 1 teaspoon of olive oil</instruction>
            <ingredients>
                <name>oliveoil</name>
                <amount>1.0</amount>
                <unit>teaspoon</unit>
            </ingredients>
        </instructions>
        <instructions>
            <instruction>Fry 1 clove of garlic in wok until golden brown</instruction>
            <cooktime>5M</cooktime>
            <ingredients>
                <name>garlic</name>
                <amount>1.0</amount>
                <unit>clove</unit>
            </ingredients>
        </instructions>
        <instructions>
            <instruction>Ease in 1 cup of leftover rice and 2 tablespoon of butter</instruction>
            <cooktime>10M</cooktime>
            <ingredients>
                <name>butter</name>
                <amount>2.0</amount>
                <unit>tablespoon</unit>
            </ingredients>
            <ingredients>
                <name>rice</name>
                <amount>1.0</amount>
                <unit>cup</unit>
            </ingredients>
        </instructions>
        <instructions>
            <instruction>Stir fry until everything is evenly mixed</instruction>
            <cooktime>5M</cooktime>
        </instructions>
        <instructions>
            <instruction>Plate beef and rice and serve!</instruction>
        </instructions>
        <tagged>Western</tagged>
        <tagged>American</tagged>
    </recipes>
